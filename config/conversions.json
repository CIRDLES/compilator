{
	"clickOn"  : "swinger.clickOn(matchingAll( {{#each selectors}}\"{{{@key}}}:{{{this}}}\"{{#unless @last}}, {{/unless}}{{/each}} ));",
	"inputText": "swinger.enterText( {{{text}}} );",
	"mouseMove": "robot.mouseMove( {{{x}}}, {{{y}}} );",
	"mouseDown": "robot.mousePress(InputEvent.BUTTON1_MASK);",
	"mouseUp"  : "robot.mouseRelease(InputEvent.BUTTON1_MASK);",
	"wait": "swinger.pause({{{wait}}});"
}